{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
